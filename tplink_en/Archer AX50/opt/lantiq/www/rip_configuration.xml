<?xml version="1.0" encoding="UTF-8"?>
<view viewid="ripconfig">
        <helptext></helptext>
    <header>
     <image>images/icons/nat.png</image>
    <name>RIP(Routing Informaton Protocol)</name>
        <description>This page allows to enable or disable RIP and set mode of operation.</description>
    </header>
    <object  viewtype="form" title= "Settings" name="Device.Routing.RIP" icon="settings-1" polling="true" interval="20000">
        <row>
        <col>
        <parameter name="Enable"  required="false" type="toggle" webname="RIP"></parameter>
        </col>
        <col>
        <parameter name="SupportedModes"  required="false" type="dropdown" validvalues="Send,Receive,Both" webname="Mode"></parameter>
        </col>
        </row>
    <parameter name="Modify" action="post" routeurl="Apply($event)" type="submitbutton" webname="Apply" /></object>
</view>

