<?xml version="1.0" encoding="UTF-8"?>
<view viewid="dyndns_server_formpage">
    <helptext></helptext>
	<header>
		<name>DYNDNS Server Settings</name>
		<description></description>
	</header>
	<object viewtype="form" name="Device.DynamicDNS.Server.*" title="Server Settings">
		<parameter name="Enable"  type="checkbox" webname="Enable" ></parameter>
		<parameter name="Name"  type="string" webname="Name" ></parameter>
		<parameter name="ServiceName"  type="string" webname="Service Name" ></parameter>
		<parameter name="ServerAddress"  type="string" webname="Server Address"/>
        <parameter  name="Add"  type="submitbutton"  routeurl="Apply($event,'dyndns')" webname="Apply"></parameter>
        <parameter  name="Close"  type="submitbutton" routeurl="Add('dyndns')" webname="Cancel"></parameter>
	</object>
</view>

