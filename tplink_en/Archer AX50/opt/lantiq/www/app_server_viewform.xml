<view viewid="app_server_viewformpage" translate="app_server_viewformpage">
<helptext></helptext>
    <header>
    	<image>images/icons/firewall.png</image>
        <name>Application Server Settings</name>
        <description>Configure a rule to allow the packets from Lan and/or Wan.</description>
    </header>
    <object title="Rule" name="Device.Firewall.X_LANTIQ_COM_ApplicationService.Rule.*"  viewtype="form">
        <parameter name="Enable"  type="checkbox" Display="No" Value="1" webname="Enable"></parameter>
		
       <parameter name="Description"  type="string" required="false"  placeholder="true" webname="Description" maxlen="64"></parameter>
		<row>
		<col>
       <parameter name="Port"  type="number" required="false"  placeholder="true" webname="Service" maxval="65535" minval="-1"></parameter>
		</col>
		<col>
       <parameter  name="Protocol" type="dropdown" validvalues="TCP,UDP,BOTH"   webname="Protocol"></parameter>
		</col>
		</row>
		<row>
		<col>
        <parameter name="AccessTraffic" type="multi-checkbox" required="false" url="app_server_form" webname="Access Service From" ></parameter>
		</col>
		</row>
        <parameter  name="Add"  type="submitbutton"  routeurl="Apply($event,'app_server_view')" webname="Save"></parameter>
        <parameter  name="Close"  type="submitbutton" routeurl="Add('app_server_view')" webname="Cancel"></parameter>
    </object>
</view>
