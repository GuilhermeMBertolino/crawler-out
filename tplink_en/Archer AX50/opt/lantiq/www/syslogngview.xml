<?xml version="1.0" encoding="UTF-8"?>
<view viewid="syslogng">
<helptext></helptext>
    <header>
	<image>images/icons/system.png</image>
        <name>System Log Configurations</name>
        <description>The CPE device provides customized logging the messages to File, Remote, Console, Telnet and Web.</description>
    </header>
    <object title="Syslog Rules" name="Device.DeviceInfo.VendorLogFile.*"  form="syslogngviewform" viewtype="table" Type="string" icon="system">
        <parameter name="X_LANTIQ_COM_Source"  type="string" writable="true" webname="Log Source" maxlen="64"></parameter>
        <parameter name="X_LANTIQ_COM_Destination"  type="string" writable="true" webname="Log Destination" maxlen="64"></parameter>
        <parameter name="Name"  type="string" writable="true" webname="File Name" maxlen="64"></parameter>
        <parameter name="MaximumSize"  type="string" writable="true" webname="File Size(in KiB)" maxlen="64"></parameter>
	<parameter name="X_LANTIQ_COM_LogFilePath"  type="string" writable="true" webname="File Location" Display="No" maxlen="128"></parameter>
        <parameter name="X_LANTIQ_COM_IpAddress"  type="string" writable="true" webname="Remote IP" maxlen="64"></parameter>
        <parameter name="X_LANTIQ_COM_Port"  type="string" writable="true" webname="Port No" maxlen="64"></parameter>
        <parameter name="X_LANTIQ_COM_LogLevel"  type="string" writable="true" webname="Log Level" maxlen="64"></parameter>
        <parameter name="X_LANTIQ_COM_FilterString"  type="string" writable="true" webname="Filter String" maxlen="64"></parameter>
        <parameter name="X_LANTIQ_COM_Program"  type="string" writable="true" webname="Program" maxlen="64"></parameter>
        <parameter name="Persistent"  type="checkbox" Display="No" webname="Enable File BackUp"></parameter>
        <parameter name="edit"  action="post" routeurl="edit($event,'syslogngviewform')" type="button" webname="Edit" maxlen="64"></parameter>
        <parameter name="view"  action="post" routeurl="view($event,'syslogtable')" type="button" webname="View" maxlen="64"></parameter>
        <parameter name="delete"  action="post"   type="button" webname="delete" maxlen="64"></parameter>
        <parameter name="Add"  action="post" routeurl="formToOpen('syslogngviewform')" type="button" webname="Add" maxlen="64"></parameter>
    </object>
</view>
