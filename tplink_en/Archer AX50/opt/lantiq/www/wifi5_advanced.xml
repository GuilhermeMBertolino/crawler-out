<?xml version="1.0" encoding="UTF-8"?>
<view>
  <header>
         <image>images/icons/wireless.png</image>
    <name>Advanced</name>
    <description>Advanced settings for the wireless LAN interfaces that operate in the 5GHz range. Typically you shouldn't need to change these settings.</description>
  </header>
  <object viewtype="form">
    <object name="Device.WiFi.Radio.2" title="Vendor Details">
      <row>
        <col>
          <parameter name="TransmitPower" type="dropdown" url="cgi_get_fillparams?Object=Device.WiFi.Radio.2&amp;TransmitPowerSupported=" webname="Power scale"/>
        </col>
      </row>
    </object>
    <object name="Device.WiFi.Radio.2.X_LANTIQ_COM_Vendor">
      <row>
        <col>
          <parameter name="WaveExplicitBeamforming" type="toggle" webname="Explicit Beamforming"/>
        </col>
        <col>
          <parameter name="WaveImplicitBeamforming" type="toggle" webname="Implicit Beamforming"/>
        </col>
      </row>
      <row>
        <col>
          <parameter name="HtLDPCenabled" type="toggle" webname="LDPC"/>
        </col>
        <col>
          <parameter name="HtSTBCenabled" type="toggle" webname="STBC"/>
        </col>
      </row>
      <row>
        <col>
          <parameter name="NumOfAntennas" type="dropdown" url="cgi_get_fillparams?Object=Device.WiFi.Radio.2.X_LANTIQ_COM_Vendor&amp;NumOfAntennasSupported=" webname="Number of Antennas"/>
        </col>
      </row>
    </object>
    <object name="Device.WiFi.AccessPoint.2">
      <parameter name="UAPSDEnable" type="toggle" webname="WMM PS"/>
    </object>
    <object name="Device.WiFi.Radio.2.X_LANTIQ_COM_Vendor">
      <parameter name="DynamicBandwidthEnabled" type="toggle" webname="Dynamic Bandwidth"/>
    </object>
    <object name="Device.WiFi.Radio.2">
      <parameter name="IEEE80211hEnabled" type="toggle" webname="DFS"/>
    </object>
    <parameter name="Modify" type="submitbutton" routeurl="Apply($event)" webname="Apply"/>
  </object>
</view>
