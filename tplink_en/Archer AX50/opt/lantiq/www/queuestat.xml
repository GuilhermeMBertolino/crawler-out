<view viewid="qosstats">
<helptext></helptext>
    <header>
    	<name>Queue Statistics</name>
        <description>Shows statistics of configured queues.</description>
    </header>
    <object name="Device.QoS.QueueStats.*" viewtype="table"  title="QosStats">
                <parameter name="X_LANTIQ_COM_Name" type="string" webname="Name" maxlen="64"></parameter>
                <parameter name="X_LANTIQ_COM_LowerLayers" type="string" webname="Interface" maxlen="64"></parameter>
                <parameter name="X_LANTIQ_COM_InputPackets" type="string" access="readonly"  webname="Input Packets" maxlen="64"></parameter>
                <parameter name="X_LANTIQ_COM_InputBytes" type="string" access="readonly"  webname="Input Bytes" maxlen="64"></parameter>
                <parameter name="OutputPackets" type="string" access="readonly"  webname="Output Packets" maxlen="64"></parameter>
                <parameter name="OutputBytes" type="string" access="readonly"  webname="Output Bytes" maxlen="64"></parameter>
                <parameter name="DroppedPackets" type="string" access="readonly" webname="Dropped Packets" maxlen="64"></parameter>
                <parameter name="DroppedBytes" type="string" access="readonly" webname="Dropped Bytes" maxlen="64"></parameter>
                <parameter name="QueueOccupancyPackets" type="string" access="readonly" webname="Queue Occupancy Packets" maxlen="64"></parameter>
                <parameter name="QueueOccupancyPercentage" type="string" access="readonly" webname="Queue Occupancy Percentage" maxlen="64"></parameter>
        </object>
</view>
