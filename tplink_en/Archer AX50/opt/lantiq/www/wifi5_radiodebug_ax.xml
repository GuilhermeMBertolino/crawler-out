<?xml version="1.0" encoding="UTF-8"?>
<view>
  <header>
         <image>images/icons/wireless.png</image>
    <name>Advanced</name>
    <description>Advanced settings for the wireless LAN interfaces that operate in the 5GHz range and support 802.11ax mode configuration. Enable via Basic->Radio.</description>
  </header>
  <object viewtype="form">
    <object name="Device.WiFi.Radio.2.X_LANTIQ_COM_Vendor" url="cgi_get_nosubobj?Object=Device.WiFi.Radio.2.X_LANTIQ_COM_Vendor">
      <row>
        <col> 
          <parameter name="HePlusHtcHeSupport" required="false" type="toggle" webname="Enable PLUS_HTC_HE"/>
          <parameter name="HeAckEnabledAggregationSupport" required="false" type="toggle" webname="Enable ACK_MULTI_TID_AGGREGATION"/>
          <parameter name="HeAMsduInAMpduSupport" required="false" type="toggle" webname="Enable A_MSDU_IN_A_MPDU SUPPORT"/>
          <parameter name="HeDeviceClass" required="false" type="toggle" webname="Enable DEVICE_CLASS_A"/>
          <parameter name="HeLdpcCodingInPayload" required="false" type="toggle" webname="Enable LPDC Coding in payload"/>
          <parameter name="HeSuBeamformerCapable" required="false" type="toggle" webname="Enable SU Beam Former Capable"/>
          <parameter name="HeMuBeamformerCapable" required="false" type="toggle" webname="Enable Mu Beam Former Capable"/>
          <parameter name="HeTriggeredSuBeamformingFeedback" required="false" type="toggle" webname="Enable Triggered SU Beam forming FeedBack"/>
          <parameter name="HeTriggeredCqiFeedback" required="false" type="toggle" webname="Enable Triggered CQI Feedback"/>
          <parameter name="HeSuPpduAndHeMuPpduWith4xHeLtfAnd08usGi" required="false" type="toggle" webname="Enable SU_PPDU_AND_HE_MU_PPDU WITH_4X_HE_LTF_AND_08US_GI"/>
        </col>
      </row>
      <row>
        <col>
            <parameter name="HeMacMaximumNumberOfFragmentedMsdusAmsdus" required="false" type="number" webname="Mac Maximum Number Of Fragmented Msdus"/>
            <parameter name="HeMacMultiTidAggregationRxSupport" required="false" type="number" webname="Mac Multi TID Aggregation Support"/>
        </col>
        <col>
            <parameter name="HeMacMaximumAMpduLengthExponent" required="false" type="number" webname="Mac Maximum A mpdu Length Exponent"/>
            <parameter name="HePhyChannelWidthSet" required="false" type="number" webname="Phy Channel Width Set"/>
            <parameter name="HePhyStbcTxLessThanOrEqual80mhz" required="false" type="number" webname="Phy Stbc Tx Less Than Or Equal 80mhz"/>
        </col>
        <col>
            <parameter name="HePhyDcmMaxConstellationTx" required="false" type="number" webname="Phy Dcm Max Constellation Tx"/>
            <parameter name="HeOperationBssColor" required="false" type="number" webname="Operation Bss Color"/>
            <parameter name="HeOperationHeDurationBasedRtsThreshold" required="false" type="number" webname="Operation Duration Based Rts Threshold"/>
        </col>
      </row>
    </object>
    <parameter name="Modify" type="submitbutton" routeurl="Apply($event)" webname="Apply"/>
  </object>
  <object viewtype="form" title="HE MU Enable">
	  <object name="Device.WiFi.Radio.2.X_LANTIQ_COM_Vendor" url="cgi_get_nosubobj?Object=Device.WiFi.Radio.2.X_LANTIQ_COM_Vendor">
		<row>
		<col>
			<parameter name="WaveHeMuOperationEnable" required="false" type="toggle" webname="Enable HE-MU (for all radios)"/>
		</col>
		</row>
	  </object>
	<parameter name="Modify" type="submitbutton" routeurl="Apply($event)" webname="Apply"/>
  </object> 
</view>