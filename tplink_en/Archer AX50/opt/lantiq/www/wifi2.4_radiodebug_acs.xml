<?xml version="1.0" encoding="UTF-8"?>
<view>
  <header>
    <image>images/icons/wireless.png</image>
    <name>Advanced</name>
    <description>Advanced settings for the wireless LAN interfaces that operate in the 2.4GHz range. Typically you shouldn't need to change these settings.</description>
  </header>
  <object viewtype="form">
    <object name="Device.WiFi.Radio.1.X_LANTIQ_COM_Vendor" url="cgi_get_nosubobj?Object=Device.WiFi.Radio.1.X_LANTIQ_COM_Vendor">
      <row>
        <col>
          <parameter name="WaveAcsUpdateTimeout" required="false" type="string" webname="ACS Update Timeout"/>
        </col>
        <col>
          <parameter name="WaveAcsChanNoisePenalty" required="false" type="string" webname="ACS Channel Noise Penalty"/>
        </col>
        <col>
          <parameter name="WaveAcsChanCustPenalty" required="false" type="string" webname="ACS Channel Noise Customer Penalty"/>
        </col>
      </row>
      <row>
          <parameter name="WaveAcsPolicy" required="false" type="toggle" webname="ACS Policy: [0-throughput] [1-reach]"/>
      </row>
      <row>
        <col>
          <parameter name="WaveAcsFallbackChannel" required="false" type="string" webname="ACS Fallback Channel [primary] [secondary] [BW]"/>
        </col>
        <col>
          <parameter name="WaveAcsToDegradation" required="false" type="string" webname="ACS To Degradation [0-32767]X6 [0-255]"/>
        </col>
        <col>
          <parameter name="WaveAcsGrpPrioritiesThroughput" required="false" type="string" webname="ACS Grp Priorities Throughput [0-3] [0-3] [0-3] [0-3]"/>
        </col>
      </row>
      <row>
        <col>
          <parameter name="WaveAcsGrpPrioritiesReach" required="false" type="string" webname="ACS Grp Priorities Reach [0-3] [0-3] [0-3] [0-3]"/>
        </col>
        <col>
          <parameter name="WaveAcsBwComparison" required="false" type="toggle" webname="ACS Bw Comparison on/off"/>
        </col>
        <col>
          <parameter name="WaveAcsBwThreshold" required="false" type="string" webname="ACS Bw Threshold [0-100] [0-100] [0-100]"/>
        </col>
      </row>
    </object>
    <parameter name="Modify" type="submitbutton" routeurl="Apply($event)" webname="Apply"/>
  </object>
</view>
