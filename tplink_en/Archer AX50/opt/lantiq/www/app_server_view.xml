<?xml version="1.0" encoding="UTF-8"?>
<view viewid="app_server_viewpage">
<helptext></helptext>
    <header>
    <image>images/icons/firewall.png</image>
        <name>Application Server Settings</name>
        <description>Application layer protocol services like Web server, Telnet server, TFTP server, FTP Server, and SNMP can be enabled/disabled from this page.</description>
    </header>
    <object title="Rules" name="Device.Firewall.X_LANTIQ_COM_ApplicationService.Rule.*" viewtype="table" icon="rules2">
        <parameter name="Enable"  type="string" webname="Enable" iconicurl="status"></parameter>
        <parameter name="Description"  type="string" writable="true" webname="Service" maxlen="64"></parameter>
        <parameter name="Port" url="app_server_port_def"   type="dropdown1" writable="true" webname="Service" maxlen="64"></parameter>
        <parameter name="Protocol"  type="string" writable="true" webname="Protocol" maxlen="64"></parameter>
        <parameter name="AccessTraffic" url="app_server_table"  type="dropdown1" writable="true" webname="Access Service From"></parameter>
        <parameter  name="Add"  action="post" routeurl="formToOpen('app_server_viewform')"  type="button" webname="Add"></parameter>
        <parameter name="edit" webname="Edit" type="button" routeurl="edit($event,'app_server_viewform')" action="post"/>
        <parameter name="delete" webname="delete" type="button" action="post"/>
    </object>
</view>
