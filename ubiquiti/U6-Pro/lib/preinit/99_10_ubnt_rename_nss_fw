rename_nss_fw() {
	[ -f /lib/firmware/qca-nss0-retail.bin ] && (cd /lib/firmware && ln -s qca-nss0-retail.bin qca-nss0.bin)
	[ -f /lib/firmware/qca-nss1-retail.bin ] && (cd /lib/firmware && ln -s qca-nss1-retail.bin qca-nss1.bin)
	rm /etc/hotplug.d/firmware/*qca-nss-fw*
}

boot_hook_add preinit_ubnt rename_nss_fw
