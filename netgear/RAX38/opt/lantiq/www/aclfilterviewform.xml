<view viewid="aclfilterviewformpage" translate="aclfilterviewformpage">
<helptext></helptext>
    <header>
    	<image>images/icons/firewall.png</image>
        <name>ACL Filter</name>
        <description>Configure a rule for denying or alllowing the packets conforming to it. </description>
    </header>
    <object title="Rule" name="Device.X_INTEL_COM_ACL.ACLEntry.*"  viewtype="form">
        <parameter name="Protocol" type="dropdown" required="false" url="acl_filter_protocol_form" webname="Protocol" ></parameter>
		<row>
		<col>
       <parameter name="SourceIP"  type="string" required="false" placeholder="true" webname="Source IP Address" maxlen="64"></parameter>
		</col>
		<col>
       <parameter name="SourceIpSubnetMask"  type="string" required="false" placeholder="true" webname="Source NetMask" maxlen="64"></parameter>
		</col>
		</row>
		<row>
		<col>
       <parameter name="SourcePort_Start"  type="number" required="false"  placeholder="true" webname="Source Port" maxval="65535" minval="-1"></parameter>
		</col>
		<col>
       <parameter name="SourcePort_End" required="false" type="number"  placeholder="true" webname="Source Port Range" maxval="65535" minval="-1"></parameter>
		</col>
		</row>
		<row>
		<col>
       <parameter name="DestIP"  type="string" required="false"  placeholder="true" webname="Destination  IP Address" maxlen="64"></parameter>
		</col>
		<col>
       <parameter name="DestIpSubnetMask"  type="string" required="false" placeholder="true"  webname="Destination Net Mask" maxlen="64"></parameter>
		</col>
		</row>
		<row>
		<col>
       <parameter name="DestPort_Start"  type="number" required="false" placeholder="true" webname="Destination Port" maxval="65535" minval="-1"></parameter>
		</col>
		<col>
       <parameter name="DestPort_End"  type="number" required="false" placeholder="Please enter valid end port number"  webname="Destination Port Range" maxval="65535" minval="-1"></parameter>
		</col>
		</row>
        <row>
        <col>
        <parameter name="SourceMACAdress" required="false" type="mac"  placeholder="true" webname="Source MAC Address"></parameter>
        </col>
        </row>
		<row>
		<col>
       <parameter name="IPVersion" type="dropdown" required="false" url="acl_filter_version" webname="IP Version" ></parameter>
		</col>
		<col>
       <parameter  name="Target" type="dropdown" validvalues="Reject,Drop"   webname="Target"></parameter>
		</col>
		</row>
		
	<row>
        <col>
	<parameter name="StartTime" required="false" webname="Start Time" type="string"/>
	<parameter name="EndTime" required="false" webname="End Time" type="string"/>
	<parameter name="DaysOfTheWeek" required="false" webname="Days Of The Week" type="multi-checkbox" url="daysofweek"/>
        </col>
	</row>
	
	
        <parameter  name="Add"  type="submitbutton"  routeurl="Apply($event,'aclfilterview')" webname="Save"></parameter>
        <parameter  name="Close"  type="submitbutton" routeurl="Add('aclfilterview')" webname="Cancel"></parameter>
    </object>
</view>
