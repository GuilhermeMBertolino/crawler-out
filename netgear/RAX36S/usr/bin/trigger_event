#!/bin/sh

set -eu

daily_fing_query() {
    #Trigger Daily fing query event triggered as a cronjob created by seal_randcron script
    d2 -c dilgeneral[0].fingDailyUpdateTrigger true
}

daily_spc_update() {
    #Update field "spcDailyUpdateTrigger" to notify csh for getting SPC service current status on daily basis
    d2 -c CircleCfgInternal[0].spcDailyUpdateTrigger true
}

"$@"
