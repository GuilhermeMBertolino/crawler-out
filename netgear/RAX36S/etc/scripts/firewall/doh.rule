#! /bin/sh

opmode=$(config get i_opmode)
if [ "$opmode" = "apmode" -o "$opmode" = "brmode" ]; then
	iptables -w -t mangle -I PREROUTING -p tcp -d 99.83.191.32 --dport 80 -j SKIPLOG
	iptables -w -t mangle -I PREROUTING -p tcp -d 99.83.191.32 --dport 443 -j SKIPLOG
	iptables -w -t mangle -I PREROUTING -p tcp -d 75.2.84.193 --dport 80 -j SKIPLOG
	iptables -w -t mangle -I PREROUTING -p tcp -d 75.2.84.193 --dport 443 -j SKIPLOG

fi
