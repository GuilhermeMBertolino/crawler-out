config VlanCfg
	option enableVlan 'false'
	option groupType 'port'

config VlanCfg 'VlanGP'
	option port1 'true'
	option port2 'true'
	option port3 'true'
	option port4 'true'
	option wifi2g 'true'
	option wifi5g 'true'

config VlanCfg 'BridgeGP'
	option port1 'false'
	option port2 'false'
	option port3 'false'
	option port4 'false'
	option wifi2g 'false'
	option wifi5g 'false'

config switch_vlan 'EtherWan'
	option name 'EtherWan'
	option ports '0t 5t'
	option vlan '1'
	option vid '10'
	option priority '0'

config switch_vlan 'Internet'
	option name 'Internet'
	option vid '0'
	option vlan '2'
	option priority '0'
	option enable_rule '1'
	option ports '1 2 3 4 6'
	option wifi2g 'true'
	option wifi5g 'true'

config switch_vlan 'Bridge'
	option name 'Bridge'
	option vlan '11'
	option enable_rule '1'
