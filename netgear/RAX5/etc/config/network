config interface 'loopback'
	option ifname 'lo'
	option proto 'static'
	option ipaddr '127.0.0.1'
	option netmask '255.0.0.0'

config interface 'lan'
	option ifname 'eth0 ra0 rax0 ra1 rax1'
	option force_link '1'
	option type 'bridge'
	option stp '0'
	option proto 'static'
	option ipaddr '192.168.1.1'
	option netmask '255.255.255.0'
	option ip6assign '64'
	option multicast_querier '0'
	option igmp_snooping '0'
	option ip6ifaceid 'eui64'

config interface 'wan'
	option ifname 'eth1'
	option proto 'dhcp'
	option metric '20'
	option mtu '1500'

config interface 'wan_v6'
	option ifname 'eth1'
	option proto 'none'
	option dnstype 'dynamic'
	option conntype 'disabled'

config interface 'pptp'
	option proto 'pptp'
	option auto '0'
	option mtu '1436'

config interface 'l2tp'
	option proto 'l2tp'
	option auto '0'
	option mtu '1428'

config switch
	option name 'switch0'
	option reset '1'
	option enable_vlan '1'

config switch_vlan 'EtherWan'
	option name 'EtherWan'
	option device 'switch0'
	option vlan '1'
	option ports '0 5'

config switch_vlan 'Internet'
	option device 'switch0'
	option enable_rule '1'
	option vlan '2'
	option name 'Internet'
	option priority '0'
	option ports '1 2 3 4 6'

config opmode
	option mode 'router'

config op_cfg 'op_ap'

config op_cfg 'op_br'

config op_cfg 'op_rt'

config inet_cfg 'inet_global'
	option wan_mode 'DHCP'
	option wan_dnstype 'dynamic'
	option wan_dns1 ''
	option wan_dns2 ''
	option mac_clone 'default'
	option mac_addr ''

config inet_cfg 'inet_ether'
	option domain_name ''
	option proto 'dhcp'
	option iptype 'dynamic'
	option mtu '1500'

config inet_cfg 'inet_pppoe'
	option server ''
	option proto 'pppoe'
	option iptype 'dynamic'
	option conn_mode 'onDemand'
	option idle_timeout '5'
	option mtu '1492'

config inet_cfg 'inet_pptp'
	option server ''
	option proto 'pptp'
	option iptype 'dynamic'
	option conn_mode 'onDemand'
	option idle_timeout '5'
	option mtu '1436'

config inet_cfg 'inet_l2tp'
	option server ''
	option proto 'l2tp'
	option iptype 'dynamic'
	option conn_mode 'onDemand'
	option idle_timeout '5'
	option mtu '1428'
