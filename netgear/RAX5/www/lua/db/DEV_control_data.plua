#!/usr/bin/lua

local json = require "luci.json"
local accessControl  = require "webGetFunc.accessControl"
local log = require("luci.log") --for debug

local enableAccessCtrl = accessControl.getAcceccControlEnable();
local accessRule = accessControl.getAcceccControlRule();

--local client_mac = accessControl.getClientMac();
--local circleActivationStatus = accessControl.getCircleActivationStatus();

local data = { };

data["enableAccessCtrl"] = { value = enableAccessCtrl, type = "checkbox" }
data["accessRule"] = { value = accessRule, type = "radio" }
--data["client_mac"] = { value = client_mac }
--data["circleActivationStatus"] = { value = circleActivationStatus }

return json.encode(data)

