#!/usr/bin/lua

local json = require "luci.json"
local wizard  = require "webGetFunc.wizard"
local interface  = require "webGetFunc.interface"
local common  = require "webGetFunc.commonCfg"

local data = { }

data["sn"] = { value = common.getSerialNumber()}
data["internet_status"] = { value =  interface.getWizardInternetStatus()}

return json.encode(data)