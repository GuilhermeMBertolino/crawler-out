<?xml version="1.0" encoding="UTF-8"?>
<view>
         <header>
		<image>images/icons/cellular.png</image>
    		<name>Cellular Wan</name>
     		<description>This page allows to configure Cellular</description>
         </header>
    <object name="Device.Cellular.Interface.*"  viewtype="table" title="Interfaces" icon="cellular">
    <parameter name="Enable"  type="string" writable="true" webname="Enable" iconicurl="status" maxLen="64"></parameter>
	<parameter name="Status"  type="string" webname="Status" maxlen="64"></parameter>
	<parameter name="Alias" type="string" writable="true" webname="Alias" maxlen="64"></parameter>
	<parameter name="IMEI" type="string" webname="IMEI" maxlen="64"></parameter>
	<parameter name="RSSI" type="string" webname="RSSI" maxlen="64"></parameter>
	<parameter  name="edit"  action="post" routeurl="edit($event,'cellularviewform')"  type="button" webname="Edit"></parameter>
    </object>
    <object name="Device.Cellular.Interface.1.Stats"  viewtype="table" title="Interface Stats" icon="cellular" polling="true" interval="10000">
    <parameter name="BytesSent"  type="string" webname="BytesSent" maxLen="64"></parameter>
	<parameter name="BytesReceived"  type="string" webname="BytesReceived" maxlen="64"></parameter>
	<parameter name="PacketsSent" type="string" webname="PacketsSent" maxlen="64"></parameter>
	<parameter name="PacketsReceived" type="string" webname="PacketsReceived" maxlen="64"></parameter>
    </object>
</view>
