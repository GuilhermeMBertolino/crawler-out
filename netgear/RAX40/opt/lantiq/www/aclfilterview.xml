<?xml version="1.0" encoding="UTF-8"?>
<view viewid="aclfilterviewpage">
<helptext></helptext>
    <header>
    <image>images/icons/firewall.png</image>
        <name>ACL Filter</name>
        <description>Configure ACL filter rule for denying the packets conforming to it.Please make sure to enable ACL Filter feature.</description>
    </header>
    	<object name="Device.X_INTEL_COM_ACL" url="cgi_get_nosubobj?Object=Device.X_INTEL_COM_ACL" viewtype="form">
    	        <parameter name="Enable" required="false" type="checkbox" webname="ACL Filter Feature" ></parameter>
    	<parameter  name="Modify"  action="post" routeurl="Apply($event)" type="submitbutton" webname="Save"></parameter>
    	<parameter  name="Cancel"  routeurl="reset()" action="post" type="submitbutton" webname="Reset"></parameter>	
    </object>
    <object title="Rules" name="Device.X_INTEL_COM_ACL.ACLEntry.*" viewtype="table" icon="rules2">
        <parameter name="Protocol" url="acl_filter_protocol_table"  type="dropdown1" writable="true" webname="Protocol"></parameter>
        <parameter name="SourceIP"  type="string" writable="true" webname="Source IP Address" maxlen="64"></parameter>
        <parameter name="SourceIpSubnetMask"  type="string" writable="true" webname="Source IP Subnet Mask" maxlen="64"></parameter>
        <parameter name="SourcePort_Start" url="acl_filter_def_val"  type="dropdown1" writable="true" webname="Source Port" maxlen="64"></parameter>
        <parameter name="SourcePort_End" url="acl_filter_def_val" type="dropdown1" writable="true" webname="Source Port Range" maxlen="64"></parameter>
        <parameter name="DestIP"  type="string" writable="true" webname="Destination IP Address" maxlen="64"></parameter>
        <parameter name="DestIpSubnetMask"  type="string" writable="true" webname="Dest IP Subnet Mask" maxlen="64"></parameter>
        <parameter name="DestPort_Start" url="acl_filter_def_val"  type="dropdown1" writable="true" webname="Destination Port" maxlen="64"></parameter>
        <parameter name="DestPort_End" url="acl_filter_def_val"  type="dropdown1" writable="true" webname="Destination Port Range" maxlen="64"></parameter>
        <parameter name="SourceMACAdress"  type="string" writable="true" webname="Source MAC" maxlen="64"></parameter>
        <parameter name="Target"  type="string" writable="true" webname="Target" maxlen="64"></parameter>
        <parameter name="StartTime"  type="string" writable="true" webname="StartTime" maxlen="64"></parameter>
        <parameter name="EndTime"  type="string" writable="true" webname="EndTime" maxlen="64"></parameter>
        <parameter name="DaysOfTheWeek"  type="string" writable="true" webname="Days Of The Week" maxlen="64"></parameter>
        <parameter  name="Add"  action="post" routeurl="formToOpen('aclfilterviewform')"  type="button" webname="Add"></parameter>
        <parameter name="edit" webname="Edit" type="button" routeurl="edit($event,'aclfilterviewform')" action="post"/>
        <parameter name="delete" webname="delete" type="button" action="post"/>
    </object>
</view>
