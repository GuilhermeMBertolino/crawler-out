<?xml version="1.0" encoding="UTF-8"?>
<view>
    <header>
        <name>Autowan Configuration</name>
        <description>This page allows you to view and configure autowan details.</description>
    </header>
    <object name="Device.X_LANTIQ_COM_AutoWan.Diagnostics.*" viewtype="form" >
    <parameter  name="Enable"  type="checkbox" required="false" webname="Enable AutoDetect" ></parameter>
    <parameter  name="Priority"  required="false" type="dropdown" validvalues="1,2,3,4,5,6" webname="Priority" ></parameter>
    <parameter  name="Wanmode"  required="false" type="string" access="readonly" webname="Wan Mode" ></parameter>
    <parameter  name="Username"  required="false" type="string" webname="Username" ></parameter>
    <parameter  name="Password"  required="false" type="password" webname="Password" ></parameter>
    <parameter  name="ProbeList"  type="string" webname="ProbeList" ></parameter>
    <parameter  name="Add"  action="post" routeurl="Apply($event,'autowan_configuration')" type="submitbutton" webname="Apply" ></parameter>
   </object>
                                                                                                                                        
</view>
