<view viewid="pptpform">
<helptext></helptext>
<header>
         <image>images/icons/tunneling.png</image>
         <name>PPTP Client</name>
        <description>This page allows you to configure PPTP FORM for your box.</description>

    </header>    
	<object viewtype="form" >
        <object name="Device.X_INTEL_COM_PPTPClient">
			<row>
				<col>
					<parameter  name="Enable"  type="checkbox"  webname="Enable" ></parameter>
			
				</col>
	</row>
		</object>
		<object name="Device.X_INTEL_COM_PPTPClient" dependsonparamvalue="1" dependsonobject="Device.X_INTEL_COM_PPTPClient" dependsonparam="Enable" >
			<row>
			<col>
				<parameter  name="Username"  type="string"  webname="Username" ></parameter>
			</col>
			<col>
				<parameter name="Password"   type="password" webname="Password" ></parameter>
			</col>	
			</row>
		<row>
			<col>
          <parameter  name="Serverip"  type="ipv4"  webname="Serverip"> </parameter>
            <parameter name="EncryptionProtocol"  type="string" webname="EncryptionProtocol" Value="" required="false"></parameter>
					</col>
			</row>
			<row>
			<col>
				<parameter name="Compression" type="string" webname="Compression" required="false" ></parameter>
				
			</col>
			</row>
            
			
			<parameter action="post" name="Modify" routeurl="Apply($event)"  type="submitbutton" webname="Apply"></parameter>
<!--			<parameter name="Close" type="submitbutton" routeurl="Add('pptppage')" webname="Cancel" maxlen="64"/>-->

        </object>
    </object>
</view>
