#!/bin/sh
#	$Id: MAKEDEV,v 1.8 2003/04/01 19:43:06 awarner Exp $

#Make Node for DB25 parallel port by stanley
mknod -m 755 lp0 c 6 0; chown 0.0 lp0
mknod -m 755 lp1 c 6 1; chown 0.0 lp1

#Make cdrom device node
mknod -m 660 scd0 b 11 0 ; chown 0.0 scd0
mknod -m 660 scd1 b 11 1 ; chown 0.0 scd1
mknod -m 660 scd2 b 11 2 ; chown 0.0 scd2
mknod -m 660 scd3 b 11 3 ; chown 0.0 scd3
mknod -m 660 scd4 b 11 4 ; chown 0.0 scd4
mknod -m 660 scd5 b 11 5 ; chown 0.0 scd5
mknod -m 660 scd6 b 11 6 ; chown 0.0 scd6
mknod -m 660 scd7 b 11 7 ; chown 0.0 scd7
mknod -m 660 scd8 b 11 8 ; chown 0.0 scd8
mknod -m 660 scd9 b 11 9 ; chown 0.0 scd9
mknod -m 660 scd10 b 11 10 ; chown 0.0 scd10
mknod -m 660 scd11 b 11 11 ; chown 0.0 scd11
mknod -m 660 scd12 b 11 12 ; chown 0.0 scd12
mknod -m 660 scd13 b 11 13 ; chown 0.0 scd13
mknod -m 660 scd14 b 11 14 ; chown 0.0 scd14
mknod -m 660 scd15 b 11 15 ; chown 0.0 scd15

mknod -m 660 ttyACM0 c 166 0
mknod -m 660 ttyACM1 c 166 1
mknod -m 660 ttyACM2 c 166 2
mknod -m 660 ttyACM3 c 166 3
mknod -m 660 ttyACM4 c 166 4
mknod -m 660 ttyACM5 c 166 5
mknod -m 660 ttyACM6 c 166 6
mknod -m 660 ttyACM7 c 166 7
mknod noz0 c 241 0; mknod noz1 c 241 1
mknod noz2 c 241 2; mknod noz3 c 241 3

mknod -m 660 ttyHS0 c 252 0
mknod -m 660 ttyHS1 c 252 1
mknod -m 660 ttyHS2 c 252 2
mknod -m 660 ttyHS3 c 252 3
mknod -m 660 ttyHS4 c 252 4
mknod -m 660 ttyHS5 c 252 5
mknod -m 660 ttyHS6 c 252 6
mknod -m 660 ttyHS7 c 252 7
mknod -m 660 scd0 b 11 0

#Make Node for usbserial device
mknod -m 660 ttyUSB0 c 188 0; chown root.uucp ttyUSB0
mknod -m 660 ttyUSB1 c 188 1; chown root.uucp ttyUSB1
mknod -m 660 ttyUSB2 c 188 2; chown root.uucp ttyUSB2
mknod -m 660 ttyUSB3 c 188 3; chown root.uucp ttyUSB3
mknod -m 660 ttyUSB4 c 188 4; chown root.uucp ttyUSB4
mknod -m 660 ttyUSB5 c 188 5; chown root.uucp ttyUSB5
mknod -m 660 ttyUSB6 c 188 6; chown root.uucp ttyUSB6
mknod -m 660 ttyUSB7 c 188 7; chown root.uucp ttyUSB7
mknod -m 660 ttyUSB8 c 188 8; chown root.uucp ttyUSB8
mknod -m 660 ttyUSB9 c 188 9; chown root.uucp ttyUSB9
mknod -m 660 ttyUSB10 c 188 10; chown root.uucp ttyUSB10
mknod -m 660 ttyUSB11 c 188 11; chown root.uucp ttyUSB11
mknod -m 660 ttyUSB12 c 188 12; chown root.uucp ttyUSB12
mknod -m 660 ttyUSB13 c 188 13; chown root.uucp ttyUSB13
mknod -m 660 ttyUSB14 c 188 14; chown root.uucp ttyUSB14
mknod -m 660 ttyUSB15 c 188 15; chown root.uucp ttyUSB15

mknod -m 660 VIAUSBModem0 c 167 0; chown root.uucp VIAUSBModem0

mknod -m 660 ttyIPWp0 c 250 0; chown 0.0 ttyIPWp0
mknod -m 660 ttyIPWp8 c 250 8; chown 0.0 ttyIPWp8

mknod -m 660 hda b 3 0 ; chown 0.0 hda
