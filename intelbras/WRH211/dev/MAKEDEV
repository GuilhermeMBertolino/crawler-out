mknod -m 660 hda2 b 3 2 ; chown 0.0 hda2
mknod -m 660 hda3 b 3 3 ; chown 0.0 hda3
mknod -m 660 hda4 b 3 4 ; chown 0.0 hda4
mknod -m 660 hda5 b 3 5 ; chown 0.0 hda5
mknod -m 660 hda6 b 3 6 ; chown 0.0 hda6
mknod -m 660 hda7 b 3 7 ; chown 0.0 hda7
mknod -m 660 hda8 b 3 8 ; chown 0.0 hda8
mknod -m 660 hdb b 3 64	; chown 0.0 hdb
